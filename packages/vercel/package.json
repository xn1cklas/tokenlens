{
  "name": "@tokenlens/vercel",
  "version": "2.0.0-alpha.1",
  "private": false,
  "description": "Tokenlens middleware helpers for the Vercel AI SDK.",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "pnpm run clean",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest --config ../../vitest.config.ts --project vercel",
    "test:run": "vitest run --config ../../vitest.config.ts --project vercel",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xn1cklas/tokenlens.git",
    "directory": "packages/vercel"
  },
  "keywords": [
    "ai",
    "llm",
    "vercel ai",
    "pricing",
    "cost estimation",
    "middleware",
    "ai sdk",
    "tokenlens"
  ],
  "author": "Nicklas Scharpff <hi@n1cklas.com> (https://n1cklas.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xn1cklas/tokenlens/issues"
  },
  "homepage": "https://github.com/xn1cklas/tokenlens/tree/HEAD/packages/vercel#readme",
  "dependencies": {
    "tokenlens": "workspace:*"
  },
  "peerDependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "ai": "^5.0.51"
  },
  "devDependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "ai": "^5.0.51",
    "msw": "^2.4.9",
    "typescript": "^5.9.2"
  }
}
